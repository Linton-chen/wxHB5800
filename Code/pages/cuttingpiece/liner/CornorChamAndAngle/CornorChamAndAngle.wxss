/* pages/cuttingpiece/liner/CornorChamAndAngle/CornorChamAndAngle.wxss */

.tip {
  color: #888;
  font-size: 14px;
  line-height: 20px;
  margin-top: 5px;
  margin-bottom: 5px;
  margin-left: 15px;
  font-family: "微软雅黑";
  padding: 0 10px;
}

.item-container {
  transition: all 0.45s cubic-bezier(0.23, 1, 0.32, 1);
  background-color: #fcfcfc;
  box-shadow: rgba(0, 0, 0, 0.117647) 0px 1px 6px, rgba(0, 0, 0, 0.117647) 0px 1px 4px;
  border-radius: 2px;
  margin: 5px 10px;
  padding: 5px;
}

button {
  margin: 10px auto;
  height: 40px;
  line-height: 40px;
  width: 80%;
}

.title {
  margin: 10px 15px;
  font-weight: bold;
}

.h1 {
  font-size: 16px;
}

.result {
  padding-left: 15px;
  margin: 5px auto;
  font-family: 'Times New Roman', Times, serif;
}
